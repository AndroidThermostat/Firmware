<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/title"
    />

    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/latency"></TextView>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/default_latency" android:id="@+id/latency"></TextView>
    </LinearLayout>
    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/throughput"></TextView>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/default_throughput" android:id="@+id/throughput"></TextView>
    </LinearLayout>

	<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/clear_button" android:text="@string/clear_button"></Button>
</LinearLayout>
